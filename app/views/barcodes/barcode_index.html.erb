 <% unless flash[:notice].nil? %><p class="flash-msg"> <%= flash[:notice] %> </p><% end %>
<div id="content-header">
  <h1>Library Management</h1>
</div>
  <div><%= image_tag("underline.jpg",width: "100%")%></div> 

<div id="page-yield">

 <div class="bread_crumb">
    <%=link_to "Home",new_user_session_path%> &raquo;<%=link_to "Library",library_management_library_dashboard_path%> &raquo;Manage Barcode
  </div>
<%=form_for :barcode, url:barcodes_manage_barcode_path ,method: :get,remote:true do |f| %>
 <p><%= f.label :Search_By%>
 <%= select_tag :search,options_for_select([["Book Number","book_number"],["Title","title"],["Author","author"],["Tag","tag"]])%>

 <%= text_field_tag :value%>
 <%= f.submit :"Search"%></p>
<%end%>
