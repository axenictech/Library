
$("ul.errors").html("")
<% if @tag.errors.any? %>
		<% @tag.errors.full_messages.each do |message| %>
		$("ul.errors").append($("<li />").html("<%= message.html_safe %>"))
		<% end %>
<% else %>
$("#show").html("<%=j render 'tag_show'%>")
$("#msg").html("Tag updated Successfully");
$("#flash-msg").show();
<%end%>